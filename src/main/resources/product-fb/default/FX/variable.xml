<?xml version="1.0" encoding="UTF-8"?>
<declare>
	<variable type="plan">
		<item name="INSURED" formula="INSURANT"/>
		<item name="AGE" formula="INSURANT.AGE"/>
		<item name="DAY" formula="INSURANT.DAY"/>
